@using Microsoft.AspNetCore.Identity
@using CHE.Data.Models
@inject SignInManager<CheUser> SignInManager
@inject UserManager<CheUser> UserManager

@if (SignInManager.IsSignedIn(User))
{
    <li><a asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">Account</a></li>
    <li><a asp-area="Identity" asp-page="/Account/Logout">Logout</a></li>
}
else
{
    <li><a asp-area="Identity" asp-page="/Account/Register">Register</a></li>
    <li><a asp-area="Identity" asp-page="/Account/Login">Login</a></li>
}
